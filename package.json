{
  "name": "nutrisensia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:clean": "for pid in $(lsof -ti:3000 2>/dev/null); do kill -9 $pid 2>/dev/null; done; sleep 1 && rm -rf .next node_modules/.cache && npm run dev",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "validate-env": "node scripts/validate-env.js",
    "test-mfa": "node scripts/test-mfa.js",
    "check-mfa": "node scripts/check-mfa-status.js",
    "cleanup-mfa": "node scripts/cleanup-mfa-factors.js",
    "deep-mfa-diagnostic": "node scripts/deep-mfa-diagnostic.js",
    "force-cleanup-mfa": "node scripts/force-cleanup-mfa.js",
    "security-test": "node scripts/security-test.js",
    "security-test:prod": "node scripts/security-test.js --target=https://nutrisensia.com",
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.1.0",
    "@analytics/simple-analytics": "^0.4.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.9",
    "@next/third-parties": "^15.5.9",
    "@radix-ui/react-accordion": "^1.2.12",
    "@supabase/ssr": "^0.6.1",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "analytics": "^0.8.19",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "flags": "^4.0.1",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "html5-qrcode": "^2.3.8",
    "lenis": "^1.3.11",
    "lucide-react": "^0.542.0",
    "motion-dom": "^12.23.23",
    "motion-utils": "^12.23.6",
    "next": "^15.5.9",
    "next-intl": "^4.7.0",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.8.0",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-joyride": "^2.9.3",
    "reading-time": "^1.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.7",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/addon-vitest": "^9.1.2",
    "@storybook/nextjs-vite": "^9.1.2",
    "@supabase/supabase-js": "^2.57.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/browser": "^3.2.4",
    "eslint": "^8",
    "eslint-config-next": "^15.5.9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^9.1.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "storybook": "^9.1.2",
    "typescript": "5.5.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
